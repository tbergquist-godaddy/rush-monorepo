[ignore]


[include]

[declarations]
.+/node_modules/.+

[libs]
tools/flow/flow-typed/

[lints]
all=error
untyped-import=off
unclear-type=off

[options]
emoji=true
esproposal.optional_chaining=enable
esproposal.nullish_coalescing=enable
exact_by_default=true
munge_underscores=true

experimental.const_params=true

types_first=true
well_formed_exports=true

[strict]
untyped-import
unclear-type