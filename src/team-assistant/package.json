{
  "name": "@tbergq/team-assistant",
  "version": "0.0.1",
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server",
    "lint": "eslint . --fix",
    "test": "jest",
    "vercel-build": "cp dist/ deploy/public/",
    "deploy": "rm -rf deploy/public && cp -R dist/ deploy/public/ && vercel deploy/",
    "manifest": "fbt-manifest --src src",
    "collect": "fbt-collect --pretty --manifest < .src_manifest.json > .source_strings.json",
    "translate": "fbt-translate --translations translations/*.json --jenkins > translatedFbts.json",
    "fbt": "rushx manifest && rushx collect && rushx translate"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.1.0",
    "webpack-dev-server": "^3.11.0",
    "@adeira/babel-preset-adeira": "^1.0.0",
    "@babel/core": "^7.11.6",
    "babel-loader": "^8.1.0",
    "babel-eslint": "^10.1.0",
    "jest": "^26.4.2",
    "@tbergq/eslint-config": "^0.0.1",
    "eslint": "^7.8.1",
    "@adeira/eslint-config": "^3.1.1",
    "@tbergq/test-utils": "^0.0.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "css-loader": "^4.3.0",
    "style-loader": "^1.2.1",
    "html-webpack-plugin": "^4.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "vercel": "^20.1.0",
    "@adeira/sx": "^0.10.0",
    "@tbergq/components": "^0.0.1",
    "react-router-dom": "^5.2.0",
    "fbt": "^0.15.0",
    "babel-plugin-fbt": "^0.15.1",
    "babel-plugin-fbt-runtime": "^0.9.12"
  }
}
