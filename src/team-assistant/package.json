{
  "name": "@tbergq/team-assistant",
  "version": "0.0.1",
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server",
    "lint": "eslint . --fix",
    "test": "jest",
    "deploy-apps": "node scripts/deploy.js",
    "manifest": "fbt-manifest --src src",
    "collect": "fbt-collect --pretty --manifest < .src_manifest.json > .source_strings.json",
    "translate": "fbt-translate --translations translations/*.json --jenkins > translatedFbts.json",
    "fbt": "rushx manifest && rushx collect && rushx translate",
    "relay": "relay-compiler",
    "relay-validate": "relay-compiler --validate",
    "get:schema": "cp ../team-assistant-gql/schema.gql ./schema.graphql",
    "start": "http-server dist/ -o -p 9000"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.1.1",
    "webpack-dev-server": "^3.11.0",
    "@adeira/babel-preset-adeira": "^1.0.0",
    "@babel/core": "^7.11.6",
    "babel-loader": "^8.1.0",
    "babel-eslint": "^10.1.0",
    "jest": "^26.4.2",
    "@tbergq/eslint-config": "^0.0.1",
    "eslint": "^7.9.0",
    "@adeira/eslint-config": "^3.1.1",
    "@tbergq/test-utils": "^0.0.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "css-loader": "^4.3.0",
    "style-loader": "^1.2.1",
    "html-webpack-plugin": "^4.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "babel-plugin-relay": "^10.0.1",
    "graphql": "^15.3.0",
    "relay-compiler": "^10.0.1",
    "relay-config": "^10.0.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "http-server": "^0.12.3",
    "@testing-library/react": "^11.0.4",
    "relay-test-utils": "^10.0.1",
    "@testing-library/user-event": "^12.1.5",
    "@svgr/webpack": "^5.4.0"
  },
  "dependencies": {
    "react": "0.0.0-experimental-94c0244ba",
    "react-dom": "0.0.0-experimental-94c0244ba",
    "vercel": "^20.1.0",
    "@adeira/sx": "^0.11.0",
    "@tbergq/components": "^0.0.1",
    "react-router-dom": "0.0.0-experimental-ffd8c7d0",
    "fbt": "^0.15.0",
    "babel-plugin-fbt": "^0.15.1",
    "babel-plugin-fbt-runtime": "^0.9.12",
    "react-relay": "0.0.0-experimental-183bdd28",
    "relay-runtime": "^10.0.1",
    "history": "^5.0.0",
    "@adeira/fetch": "^1.0.3",
    "react-hook-form": "^6.8.3",
    "yup": "^0.29.3",
    "@hookform/resolvers": "^0.1.1",
    "recoil": "^0.0.10"
  }
}
